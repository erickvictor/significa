rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o App.cmi : cp ../bs/src/App.cmi
o App.cmj : cp ../bs/src/App.cmj
o App.cmt : cp ../bs/src/App.cmt
o App.res : cp ../../src/App.res
o Home.cmi : cp ../bs/src/pages/Home.cmi
o Home.cmj : cp ../bs/src/pages/Home.cmj
o Home.cmt : cp ../bs/src/pages/Home.cmt
o Home.res : cp ../../src/pages/Home.res
build install.stamp : touch App.cmi App.cmj Home.cmi Home.cmj 
